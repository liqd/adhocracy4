{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "extends": [
    "standard",
    "standard-jsx"],
  "rules": {
    "jsx-quotes": [2, "prefer-double"]
  },
  "overrides": [
    {
      "files": [
        "**/*.jest.js",
        "**/*.jest.jsx"
      ],
      "env": {
        "jest": true
      }
    }
  ]
}
